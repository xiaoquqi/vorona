<div id="selected_kpi">
  <% unless @selected_kpis.nil? %>
    <ul>
      已经选择的KPI指标
      <% @selected_kpis.each do |select_kpi| %>
        <li>
        <%= select_kpi.kpi_group.name %>-><%= select_kpi.human_name %>
        </li>
      <% end %>
    </ul>
  <% else %>
    请从左侧列表选择KPI指标
  <% end %>
</div>
